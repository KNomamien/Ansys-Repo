# Ignore Terraform state files
*.tfstate
*.tfstate.backup
*.tfstate.tmp
*.tfstate.migrate

# Ignore .terraform directory generated by 'terraform init'
.terraform/

# Ignore .terraform.lock.hcl file created by 'terraform init'
.terraform.lock.hcl

# Ignore override files
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Ignore any file ending with ".auto.tfvars"
*.auto.tfvars

# Ignore any file ending with ".tfvars.json"
*.tfvars.json

# Ignore crash log files
crash.log

# Ignore .tfplan files
*.tfplan

# Ignore .terraformrc file
.terraformrc

# Ignore CLI configuration files
terraform.rc
